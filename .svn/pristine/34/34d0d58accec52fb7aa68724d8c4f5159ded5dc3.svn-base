package cn.com.gome.logic.model;

import java.io.Serializable;

/**
 * 附件
 */
public class Attachment implements Serializable {
	private static final long serialVersionUID = 1L;

	private String id;//附件id
	private String attachName;// 附件名称
	private int attachType;// 附件类型
	private String attachUrl;// 附件url
	private int attachSize;// 附件大小
	private int width;
	private int height;
	private int attachPlaytime;// 播放时间
	private long attachUploadtime;
	private String extra;

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getAttachName() {
		return attachName;
	}

	public void setAttachName(String attachName) {
		this.attachName = attachName;
	}

	public int getAttachType() {
		return attachType;
	}

	public void setAttachType(int attachType) {
		this.attachType = attachType;
	}

	public String getAttachUrl() {
		return attachUrl;
	}

	public void setAttachUrl(String attachUrl) {
		this.attachUrl = attachUrl;
	}

	public int getAttachSize() {
		return attachSize;
	}

	public void setAttachSize(int attachSize) {
		this.attachSize = attachSize;
	}

	public int getWidth() {
		return width;
	}

	public void setWidth(int width) {
		this.width = width;
	}

	public int getHeight() {
		return height;
	}

	public void setHeight(int height) {
		this.height = height;
	}

	public int getAttachPlaytime() {
		return attachPlaytime;
	}

	public void setAttachPlaytime(int attachPlaytime) {
		this.attachPlaytime = attachPlaytime;
	}

	public long getAttachUploadtime() {
		return attachUploadtime;
	}

	public void setAttachUploadtime(long attachUploadtime) {
		this.attachUploadtime = attachUploadtime;
	}

	public String getExtra() {
		return extra;
	}

	public void setExtra(String extra) {
		this.extra = extra;
	}

}
